<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="seq_hosp_patient" model="ir.sequence">
            <field name="name">Hospital Patient</field>
            <field name="code">hospital.patient</field>
            <field name="prefix">AMH/</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>

    <data>
        <record id="seq_hosp_appointment" model="ir.sequence">
            <field name="name">Appointment</field>
            <field name="code">hospital.appointment</field>
            <field name="prefix">AP/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
        </record>
    </data>
</odoo>